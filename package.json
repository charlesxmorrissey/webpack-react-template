{
  "name": "webpack-react-template",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Charles X. Morrissey",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.11.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "next",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "jest": "^22.4.3",
    "lint-staged": "^8.1.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-custom-media": "^7.0.7",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-font-family-system-ui": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-nesting": "^7.0.0",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.5.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.12.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.config.dev.js",
    "start": "webpack-dev-server --open --progress --config build/webpack.config.dev.js",
    "build": "webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.jsx build src",
    "format": "prettier --write 'src/**/*.{js,jsx,json,css}'",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "coverageDirectory": ".jest_coverage",
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/build/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "snapshotSerializers": []
  }
}
