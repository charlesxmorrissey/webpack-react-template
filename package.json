{
  "name": "webpack-react-template",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Charles X. Morrissey",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "next",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "prettier": "^1.12.1",
    "style-loader": "^0.21.0",
    "styled-components": "^3.2.6",
    "system-ui": "^1.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config build/webpack.config.dev.js",
    "start": "NODE_ENV=development webpack-dev-server --open --progress --config build/webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "lint": "eslint --ext .js,.jsx build src",
    "format": "prettier --write '**/*.{js,jsx,css}'",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "**/*.{js,jsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "coverageDirectory": ".jest_coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/build/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "snapshotSerializers": []
  }
}
